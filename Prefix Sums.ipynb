   "source": [
    "def prefix_sums(arr, left, right):\n",
    "    n = len(arr)\n",
    "    result = [0] * (n + 1)\n",
    "\n",
    "    for i in range(n):\n",
    "        result[i + 1] = arr[i] + result[i]\n",
    "\n",
    "    return result[right + 1] - result[left]"
   ]
